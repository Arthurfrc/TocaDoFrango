{
  "expo": {
    "name": "Toca Do Frango",
    "slug": "toca-do-frango",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF0000"
    },
    "platforms": [
      "android",
      "ios"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
        "translucent": false
      },
      "statusBar": {
        "backgroundColor": "#FFFFFF",
        "style": "dark"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.arthurfrc.tocadofrango"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/auth",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "f8a44716-bcc2-4301-af52-1b0947609efa"
      }
    }
  }
}
